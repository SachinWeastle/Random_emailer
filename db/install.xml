<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="local/randomemail/db" VERSION="2020081500" COMMENT="XMLDB file for Moodle local/randomemail"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
<TABLES>
	<TABLE NAME="local_randomemail_users" COMMENT="Random scheduled user list">
		 <FIELDS>
		  	<FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
		  	<FIELD NAME="firstname" TYPE="text" LENGTH="big"  NOTNULL="true" SEQUENCE="false"/>
		  	<FIELD NAME="lastname" TYPE="text" LENGTH="big"  NOTNULL="false" SEQUENCE="false"/>
			<FIELD NAME="email" TYPE="text" LENGTH="big"  NOTNULL="true" SEQUENCE="false"/>
			<FIELD NAME="mailsent" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
		  	<FIELD NAME="createdtime" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false"/>
		  	<FIELD NAME="updatedtime" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false"/>
		 </FIELDS>
 		<KEYS>
	        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
	    </KEYS>
	</TABLE>
</TABLES>
</XMLDB>

